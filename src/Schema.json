{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": {
    "type": "object",
    "properties": {
      "bills": { "type": "string", "pattern": "^\\d+$" },
      "creditCard": { "type": "string", "pattern": "^\\d+$" },
      "goal": { "type": "string", "pattern": "^\\d+$" },
      "groceries": { "type": "string", "pattern": "^\\d+$" },
      "monthly_income": { "type": "string", "pattern": "^\\d+$" },
      "other": { "type": "string", "pattern": "^\\d+$" },
      "rent": { "type": "string", "pattern": "^\\d+$" },
      "timeline": { "type": "string", "pattern": "^\\d+$" },
      "total": { "type": "string", "pattern": "^\\d+(\\.\\d{1,2})?$" }
    },
    "required": ["bills", "creditCard", "goal", "groceries", "monthly_income", "other", "rent", "timeline", "total"]
  }
}

